<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gray.rxjavademo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SimpleActivity"
            android:label="@string/simple"/>
        <activity android:name=".CreateOperatorActivity"
            android:label="@string/create_operator"/>
        <activity android:name=".TransformOperatorActivity"
            android:label="@string/transform_operator"/>
        <activity android:name=".UnionOperatorActivity"
            android:label="@string/union_operator"/>
    </application>

</manifest>